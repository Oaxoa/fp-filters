{
  "name": "fp-filters",
  "version": "0.1.1",
  "description": "Commonly used filter functions that are tiny, tested and combinable",
  "author": "Pierluigi Pesenti",
  "license": "MIT",
  "type": "module",
  "engines": {
    "node": "16.14.0"
  },
  "devDependencies": {
    "@types/jest": "^29.5.2",
    "eslint": "^8.43.0",
    "jest": "^29.5.0",
    "np": "^8.0.4",
    "prettier": "^2.8.8",
    "ts-jest": "^29.1.0",
    "typescript": "^5.1.3"
  },
  "scripts": {
    "lint": "eslint",
    "test": "jest --coverage",
    "test:watch": "jest --watchAll",
    "build": "tsc --project tsconfig.build.json",
    "release": "np"
  },
  "jest": {
    "preset": "ts-jest",
    "testEnvironment": "node",
    "transform": {
      "^.+\\.ts?$": "ts-jest"
    },
    "transformIgnorePatterns": [
      "<rootDir>/node_modules/"
    ]
  },
  "exports": {
    ".": "./filters/*.js",
    "./utils": "./utils.js"
  },
  "files": [
    "./lib", "CHANGELOG.md"
  ],
  "np": {
    "contents": "./lib"
  }
}
