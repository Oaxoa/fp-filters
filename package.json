{
  "name": "fp-filters",
  "version": "0.3.3",
  "type": "module",
  "description": "Curated collection of commonly used filter functions that are tiny, tested and composable",
  "author": "Pierluigi Pesenti",
  "license": "MIT",
  "repository": "https://github.com/Oaxoa/fp-filters",
  "files": [
    "dist/",
    "assets/"
  ],
  "exports": {
    "./array": {
      "import": "./dist/esm/filters/array.js",
      "require": "./dist/cjs/filters/array.js",
      "types": "./dist/esm/filters/array.d.ts"
    },
    "./boolean": {
      "import": "./dist/esm/filtelllrs/boolean.js",
      "require": "./dist/cjs/filters/boolean.js",
      "types": "./dist/esm/filters/boolean.d.ts"
    },
    "./date": {
      "import": "./dist/esm/filters/date.js",
      "require": "./dist/cjs/filters/date.js",
      "types": "./dist/esm/filters/date.d.ts"
    },
    "./length": {
      "import": "./dist/esm/filters/length.js",
      "require": "./dist/cjs/filters/length.js",
      "types": "./dist/esm/filters/length.d.ts"
    },
    "./misc": {
      "import": "./dist/esm/filters/misc.js",
      "require": "./dist/cjs/filters/misc.js",
      "types": "./dist/esm/filters/misc.d.ts"
    },
    "./number": {
      "import": "./dist/esm/filters/number.js",
      "require": "./dist/cjs/filters/number.js",
      "types": "./dist/esm/filters/number.d.ts"
    },
    "./object": {
      "import": "./dist/esm/filters/object.js",
      "require": "./dist/cjs/filters/object.js",
      "types": "./dist/esm/filters/object.d.ts"
    },
    "./position": {
      "import": "./dist/esm/filters/position.js",
      "require": "./dist/cjs/filters/position.js",
      "types": "./dist/esm/filters/position.d.ts"
    },
    "./string": {
      "import": "./dist/esm/filters/string.js",
      "require": "./dist/cjs/filters/string.js",
      "types": "./dist/esm/filters/string.d.ts"
    },
    "./type": {
      "import": "./dist/esm/filters/type.js",
      "require": "./dist/cjs/filters/type.js",
      "types": "./dist/esm/filters/type.d.ts"
    }
  },
  "dependencies": {
    "fp-booleans": "0.5.2"
  },
  "devDependencies": {
    "@eslint/js": "9.24.0",
    "@types/jest": "29.5.14",
    "@types/bun": "1.2.9",
    "acorn": "8.14.1",
    "acorn-walk": "8.3.4",
    "bun": "1.2.8",
    "eslint": "9.24.0",
    "eslint-config-prettier": "10.1.1",
    "eslint-plugin-import": "2.31.0",
    "eslint-plugin-tree-shaking": "1.12.2",
    "jest": "29.7.0",
    "prettier": "3.5.3",
    "ts-jest": "29.3.1",
    "typescript": "5.8.3",
    "typescript-eslint": "8.29.0"
  },
  "scripts": {
    "build:esm": "bun build index.ts --outdir=dist-bun/esm --format=esm --target=node",
    "build:cjs": "bun build index.ts --outdir=dist-bun/cjs --format=cjs --target=node",
    "build": "npm run build:esm && npm run build:cjs",
    "clean": "rm -rf dist",
    "build:ts:cjs": "tsc -p tsconfig.cjs.json",
    "build:ts:esm": "tsc -p tsconfig.esm.json",
    "build:ts": "npm run clean && npm run build:ts:cjs && npm run build:ts:esm",
    "lint": "eslint .",
    "pack": "npm run --cwd ./dist pack",
    "prettier": "prettier '**/*.ts'",
    "prettier:fix": "prettier --write '**/*.ts'",
    "test": "jest --coverage",
    "test:watch": "jest --watchAll"
  },
  "keywords": [
    "filter",
    "filter functions",
    "filters",
    "filtering",
    "common filters",
    "common filter functions",
    "curated common filter functions",
    "fp filters",
    "functional programming filters",
    "functional programming",
    "typescript",
    "ts",
    "functional filters",
    "data-manipulation",
    "utility functions",
    "array filters",
    "functional utilities",
    "curated filters",
    "fp",
    "lambda functions",
    "high order functions",
    "code reusability",
    "collection processing",
    "data filtering",
    "streamlined code",
    "fp ts",
    "functional style",
    "immutable functions"
  ]
}
